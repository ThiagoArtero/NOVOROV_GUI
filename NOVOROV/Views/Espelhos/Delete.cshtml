@model NOVOROV.Models.Espelho

@{
    ViewData["Title"] = "Delete";
}




<h3>Tem certeza que deseja apagar este espelho?</h3>
<div>
    <h4>Espelho</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AnalistaCriador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AnalistaCriador.UsuarioId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomeEspelho)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeEspelho)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataEspelho)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataEspelho)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataEnvioFinanceiro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataEnvioFinanceiro)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataEnvioAnalise)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataEnvioAnalise)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>
