@model NOVOROV.Models.TipoOcorrenciaTipoSite

@{
    ViewData["Title"] = "Delete";
}
<h3>Tem certeza que deseja apagar este tipo ocorrência?</h3>
<a asp-action="Index">Voltar</a>
<div>
    <h4>TipoOcorrenciaTipoSite</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TipoSite)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoSite.NomeTipoSite)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TipoOcorrencia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoOcorrencia.NomeTipoOcorrencia)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> 
        
    </form>
</div>
